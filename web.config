<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="Node" stopProcessing="true">
                    <match url="^(.*)" />
                    <action type="Rewrite" url="http://localhost:3000/{R:1}" />
                </rule>
                <rule name="HHH">
                    <match url="^(.*)" />
                    <action type="Rewrite" url="http://localhost:3000/{R:1}" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>